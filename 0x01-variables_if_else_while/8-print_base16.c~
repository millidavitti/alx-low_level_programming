#include <stdio.h>
/**
  *main - prints all the numbers of base 16 in lowercase, followed by a new line
  *
  *Return: int
  */

int main(void)
{

int str = 97;
int num = 48;

while (str < 103)
{

if (num < 58)
{
putchar(num);
num++;
}
else if (str < 103)
{
putchar(str);
str++;
}
}
putchar('\n');

return (0);
}
